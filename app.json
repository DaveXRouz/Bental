{
  "expo": {
    "name": "Financial Advisor",
    "slug": "financial-advisor",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "ADMIN_ACCESS_TOKEN": "${EXPO_PUBLIC_ADMIN_ACCESS_TOKEN}",
      "CALENDLY_URL": "${EXPO_PUBLIC_CALENDLY_URL}"
    }
  }
}
